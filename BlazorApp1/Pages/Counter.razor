@page "/counter"

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

@for(int iterATOR = 1; iterATOR <=20; iterATOR++)
{
    if(iterATOR % 5 == 0)
    {
        <button class="btn btn-primary">@iterATOR</button>
        <br />
    }
    else
    {
        <button class="btn btn-primary">@iterATOR</button>
    }
}



@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
